{
  "name": "@globallogicuki/backstage-plugin-unleash-backend",
  "version": "0.1.5",
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./permissions": "./src/permissions/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "permissions": [
        "src/permissions/index.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.cjs.js",
        "default": "./dist/index.cjs.js"
      },
      "./permissions": {
        "types": "./dist/permissions/index.d.ts",
        "require": "./dist/permissions/index.cjs.js",
        "default": "./dist/permissions/index.cjs.js"
      },
      "./package.json": "./package.json"
    },
    "typesVersions": {
      "*": {
        "permissions": [
          "dist/permissions/index.d.ts"
        ]
      }
    }
  },
  "homepage": "https://github.com/globallogicuki/globallogic-backstage-plugins/tree/main/plugins/unleash-backend",
  "bugs": {
    "url": "https://github.com/globallogicuki/globallogic-backstage-plugins/issues"
  },
  "repository": {
    "type": "git",
    "url": "github:globallogicuki/globallogic-backstage-plugins",
    "directory": "plugins/unleash-backend"
  },
  "configSchema": "config.d.ts",
  "backstage": {
    "role": "backend-plugin",
    "pluginId": "unleash",
    "pluginPackages": [
      "@globallogicuki/backstage-plugin-unleash",
      "@globallogicuki/backstage-plugin-unleash-backend",
      "@globallogicuki/backstage-plugin-unleash-common"
    ]
  },
  "scripts": {
    "start": "backstage-cli package start",
    "build": "backstage-cli package build",
    "lint": "backstage-cli package lint",
    "test": "backstage-cli package test",
    "clean": "backstage-cli package clean",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack"
  },
  "dependencies": {
    "@backstage/backend-defaults": "^0.13.1",
    "@backstage/backend-plugin-api": "^1.5.0",
    "@backstage/catalog-client": "^1.12.1",
    "@backstage/catalog-model": "^1.7.6",
    "@backstage/errors": "^1.2.7",
    "@backstage/plugin-auth-node": "^0.6.6",
    "@backstage/plugin-catalog-node": "^1.20.0",
    "@backstage/plugin-permission-common": "^0.9.3",
    "@backstage/plugin-permission-node": "^0.10.3",
    "@globallogicuki/backstage-plugin-unleash-common": "^0.1.5",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "node-fetch": "^2.6.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@backstage/backend-test-utils": "^1.10.0",
    "@backstage/cli": "^0.34.5",
    "@types/express": "^4.17.6",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.2.4"
  },
  "files": [
    "dist",
    "config.d.ts"
  ]
}
